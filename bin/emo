#!/bin/sh

ROOT=`dirname $0`
TMP="$ROOT/tmp.js"

cat "$ROOT/../runtime/runtime.js"
"$ROOT/../sweet.js/bin/sjs" \
    -m "$ROOT/../macros/poop.js" \
    -m "$ROOT/../macros/async.js" \
    -m "$ROOT/../macros/algebra.js" \
    -m "$ROOT/../macros/error.js" \
    -m "$ROOT/../macros/terminating-cat.js" "$1"
